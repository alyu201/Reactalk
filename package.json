{
  "name": "reactalk",
  "displayName": "",
  "description": "",
  "version": "0.0.1",
  "publisher": "reactalk",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:reactalk.helloWorld",
    "onCommand:reactalk.start",
    "onCommand:reactalk.startListening",
    "onCommand:reactalk.commandProcess"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reactalk.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "reactalk.start",
        "title": "Reactalk start"
      },
      {
        "command": "reactalk.startListening",
        "title": "Reactalk start listening"
      },
      {
        "command": "reactalk.commandProcess",
        "title": "Process command"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@google-cloud/speech": "^4.10.2",
    "node-record-lpcm16": "^1.0.1"
  }
}
